<%= form_tag("/users", method: "post") do %>

	<%= label_tag "Email:" %>
	<%= text_field_tag "user[email]" %><br>

	<%= label_tag "Username:" %>
	<%= text_field_tag "user[username]" %><br>

	<%= label_tag "Password:" %>
	<%= text_field_tag "user[password]" %><br>
  
  	<%= submit_tag 'Submit' %>

<% end %>